{
  "compilerOptions": {
    "noImplicitAny": false,
    "module": "commonjs",
    "target": "es5",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "moduleResolution": "node",
    "outDir": "./build"
  },
  "exclude": [
    "node_modules",
    "bundles"
  ]
}
